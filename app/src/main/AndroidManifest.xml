<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wims.whereismystore">

    <uses-permission android:name="android.permission.CAMERA" /><!--카메라 권한-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /><!--파일 읽기 권한-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /><!--파일 쓰기 권한-->
    <uses-feature android:name="android.hardware.camera" android:required="true"/><!--카메라 기능 사용-->
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name = "com.wims.whereismystore.Class.Users"
        android:usesCleartextTraffic="true"
        >

        <activity android:name="com.wims.whereismystore.Activity.LoginActivity"
            tools:ignore="Instantiatable">

        </activity>

        <activity android:name="com.wims.whereismystore.Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.wims.whereismystore.Activity.JoinActivity">
            tools:ignore="Instantiatable">
        </activity>

        <activity android:name="com.wims.whereismystore.Activity.SaleActivity"
            tools:ignore="Instantiatable">
        </activity>
        <activity android:name="com.wims.whereismystore.Activity.SaleUploadActivity"
            tools:ignore="Instantiatable">
        </activity>
        <activity android:name="com.wims.whereismystore.Activity.WebViewActivity"
            tools:ignore="Instantiatable">
        </activity>
        <activity android:name="com.wims.whereismystore.Activity.Fragments.Fragment1"
            tools:ignore="Instantiatable">
        </activity>

        <activity android:name="com.wims.whereismystore.Activity.Fragments.Fragment2"
            tools:ignore="Instantiatable">
        </activity>

        <activity android:name="com.wims.whereismystore.Activity.Fragments.Fragment3"
            tools:ignore="Instantiatable">
        </activity>

        <activity android:name="com.wims.whereismystore.Activity.Fragments.Fragment4"
            tools:ignore="Instantiatable">
        </activity>

        <activity android:name="com.wims.whereismystore.Activity.Fragments.Fragment5"
            tools:ignore="Instantiatable">
        </activity>


    </application>
</manifest>